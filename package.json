    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-godaddy": "^2.1.0",
    "eslint-config-godaddy-es5": "^2.1.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^4.12.1",
